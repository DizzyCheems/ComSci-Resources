import java.util.LinkedList;
public class NodeIteration {
	public static void main(String[] args)
	{
		LinkedList<String> linkedList = new LinkedList<>();
		linkedList.add("Justine");
		linkedList.add("M.");
		linkedList.add("Favia");
		display(linkedList);
	}

	public static void display(LinkedList<String> linkedList)
	{
        
        System.out.print("Iterating the LinkedList using get and for loop : ");
		for (int i = 0; i < linkedList.size(); i++) {

			System.out.print(linkedList.get(i) + " ");
		}

		System.out.println();
		System.out.print("Iterating the LinkedList using enhanced for loop (foreachloop) : ");
		for (String listElement : linkedList) {
			System.out.print(listElement + " ");
		}
	}
}


